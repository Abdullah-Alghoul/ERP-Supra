<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<record id='customer_invoice_supra' model='ir.ui.view'>
			<field name="name">customer.invoice.supra</field>
			<field name='model'>account.invoice</field>
			<field name='type'>form</field>
			<field name='inherit_id' ref='ad_account_finance.customer_invoice_supra'/>
			<field name='arch' type='xml'>
				<field name='faktur_pajak_no' position='replace'>
				</field>

				<field name='kwitansi' position='replace'>
				</field>
			</field>
		</record>

		<!-- <record id='kwitansi2' model='ir.ui.view'>
			<field name="name">kwitansi2</field>
			<field name='model'>account.invoice</field>
