					<!-- <attribute name="onchange">onchange_date_invoice()</attribute> -->
					<field name="date_invoice" on_change="onchange_date_invoice(date_invoice)" />
					<field name="tax_period" />
				</field>
			</field>
		</record>
	</data>
</openerp>