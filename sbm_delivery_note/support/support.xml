<openerp>
	<data>
		<record id="inherit_material_line_for_op_lines" model="ir.ui.view">
			<field name="name">inherit material line for op lines</field>
			<field name="model">sale.order</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="new_sbm_sale_order.quotation_form_view"/>
			<field name="arch" type="xml">
				<field name="qty" position='after'>
					<field name="on_process_qty"/>
					<field name="shipped_qty"/>
