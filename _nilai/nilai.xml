<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>


		<record model='ir.actions.act_window' id="action_nilai">
			<field name="name">Nilai Wizard</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">wizard.nilai</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="view_id" ref="nilai_wizard"/>
			<field name="target">new</field>
		</record>


		
		<record id="nilai_form" model='ir.ui.view'>
			<field name='name'>nilai.form</field>
			<field name='model'>student</field>
			<field name='type'>form</field>
			<field name='inherit_id' ref="_Student.student_form_view"/>
			<field name='arch' type='xml'>
				<header position='inside'>
					<button name='%(action_nilai)d' type='action' string='Nilai' />
				</header>
				<sheet position="inside">
					<group>
						<notebook>
							<page string="Nilai">
								<field name="nilai_ids">
									<form string="Form Nilai" version="7.0">
										<sheet>
											<group>
												<field name="matakuliah" />
												<field name="nilai" />
											</group>
										</sheet>
									</form>
									<tree>
										<field name="matakuliah" />
										<field name="nilai" />
									</tree>
								</field>
							</page>
						</notebook>
					</group>
				</sheet>
			</field>
			
		</record>

		
	</data>
</openerp>